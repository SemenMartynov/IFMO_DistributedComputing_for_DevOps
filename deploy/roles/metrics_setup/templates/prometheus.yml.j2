global:
  scrape_interval: 15s
  evaluation_interval: 15s

scrape_configs:
  - job_name: 'cadvisor'
    static_configs:
      - targets: ['{{ metrics_setup_cadvisor_container }}:8080']
  
  - job_name: 'mysql_exporter'
    static_configs:
      - targets: ['{{ metrics_setup_mysql_container }}:9104']
