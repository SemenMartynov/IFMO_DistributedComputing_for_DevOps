global:
  scrape_interval: 15s

scrape_configs:
  - job_name: 'mysql_master'
    static_configs:
      - targets: ['mysql_exporter_master:9104']

  - job_name: 'mysql_slave'
    static_configs:
      - targets: ['mysql_exporter_slave:9104']

  - job_name: 'prometheus'
    static_configs:
      - targets: ['localhost:9090']